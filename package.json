{
  "name": "react-within-client-context",
  "version": "1.0.0",
  "description": "High-order component to render specific component in isomorphic application only on client-side",
  "main": "dist/index.js",
  "directories": {
    "lib": "./dist"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DzenisevichK/react-within-client-context.git"
  },
  "keywords": [
    "react",
    "high-order component",
    "isomorphic",
    "browser",
    "client"
  ],
  "author": "DzenisevichK",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/DzenisevichK/react-within-client-context/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/DzenisevichK/react-within-client-context/issues"
  },
  "homepage": "https://github.com/DzenisevichK/react-within-client-context#readme",
  "dependencies": {
    "hoist-non-react-statics": "^1.2.0",
    "react": "^15.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "rimraf": "^2.5.4"
  }
}
